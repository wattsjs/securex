        "unique_name": "definition_activity_01S25O23CR67W54V25cJwi0ahClII8l9kEw",
        "name": "Execute Python Script",
        "title": "Build the JSON payload",
        "type": "python3.script",
        "base_type": "activity",
        "properties": {
          "action_timeout": 180,
          "continue_on_failure": false,
          "display_name": "Build the JSON payload",
          "script": "import sys,json\nimport urllib.parse\n\nBASE_URI = \"https://quickchart.io/chart?c=\"\n\n# Make sure a message was provided\nhasMessage = False\nif sys.argv[4] != \"\":\n    hasMessage = True\n\n# Encode the chart data\nchart_data = urllib.parse.quote(sys.argv[4])\n\nchart_uri = BASE_URI + chart_data\n\n# Start building the JSON object to send to Webex\nwebexObject = {\n  \"files\": chart_uri,\n\t\"roomId\": sys.argv[1],\n  \"text\": \"\",\n  \"markdown\": \"\"\n}\n\nif sys.argv[2] and not sys.argv[3]:\n    webexObject[\"text\"] = sys.argv[2]\nif sys.argv[3]:\n    webexObject[\"markdown\"] = sys.argv[3]\n\n# If any new lines were escaped, unescape them (or they won't work)\nwebexObject[\"text\"] = webexObject[\"text\"].replace(\"\\\\n\", \"\\n\")\nwebexObject[\"markdown\"] = webexObject[\"markdown\"].replace(\"\\\\n\", \"\\n\")\n\n# Encode the payload to JSON\nwebexPayload = json.dumps(webexObject)",
          "script_arguments": [
            "$workflow.definition_workflow_01S25O1ZHRXYZ50BdWGSBFuql0mSJjfrBKd.input.variable_workflow_01S25O1ZRYNA61gCzmzpq7Xmx5KRtlxgRsI$",
